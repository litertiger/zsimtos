  Wb.warn('没有箱子！');\
  return;\
}\
\
var cntrs = eval(\"([])\");\
Wb.each(store.data.items, function(item) {\
  var arr = {\
    \"CNTR_ID\": item.data.ID,\
    \"CNTR\": item.data.CNTR\
  };\
  cntrs.push(arr);\
});\
\
if (app.isedit) {\
  Wb.request({\
    url: 'm?xwl=controlManage/APPLY/cntrApplyedit/update',\
    out: app.panelApply,\
    params: {\
      cntrs: cntrs\
    },\
    success: function(resp) {\
      app.btnsel.fireEvent(\"click\");\
      app.applyWin.close();\
    }\
  });\
} else {\
  Wb.request({\
    url: 'm?xwl=controlManage/APPLY/CNTRAPPLYEDIT/insert',\
    out: app.panelApply,\
    params: {\
      cntrs: cntrs\
    },\
    success: function(resp) {\
      app.btnsel.fireEvent(\"click\");\
      app.applyWin.close();\
    }\
  });\
}"}
