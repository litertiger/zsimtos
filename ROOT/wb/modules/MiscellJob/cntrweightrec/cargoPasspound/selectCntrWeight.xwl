  \"   (select TRAY_WGT  from  C_TRAY where TRAY_NO='\" + TRAY_NO + \"' and  TENANCY_ID='\" + TENANCY_ID + \"') TRAY_WGT ,\" +\
  \"  (select WEIGHT  from  C_CNTR where CNTR={?cntr?}) WEIGH  from  dual \", {\
    jndi: \"jdbc/basecode\"\
  });\
if (rm.next()) {\
