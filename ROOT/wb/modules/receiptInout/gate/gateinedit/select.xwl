if (data.shouti == \"ti\")\
  sql = \"select R.ID RID,P.ID PID,R.IN_CAR,R.IN_CAMPI,R.IN_DRIVER_ID,R.CNTR,R.CNTR_SIZE,R.CNTR_TYPE,R.SCALE_WEIGHT,\" +\
