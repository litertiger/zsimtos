    //发送微信消息\
    app.sendMessage(\"qu_wechat\", '{\"ID\":\"600015\",\"MESSAGE\":\"' + message + '\",\"DIRVER_NO\":\"' + upd.IN_DRIVER_ID + '\"}');\
  }\
});\
