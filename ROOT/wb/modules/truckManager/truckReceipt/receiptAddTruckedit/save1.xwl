    var now = new Date();\
    var year = now.getFullYear();\
    var month = now.getMonth() + 1;\
    var day = now.getDate();\
    var h = now.getHours().toString();\
    var pstime = \"\";\
    if (h.length == 1) {\
      h = \"0\" + h;\
    }\
    var m = now.getMinutes().toString();\
    if (m.length == 1) {\
