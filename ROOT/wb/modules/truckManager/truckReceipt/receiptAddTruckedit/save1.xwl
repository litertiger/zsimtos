    }\
    var s = now.getSeconds().toString();\
    if (s.length == 1) {\
      s = \"0\" + s;\
    }\
    if (!Wb.isEmpty(upd.PLAN_STIME))\
      pstime = upd.PLAN_STIME;\
    else\
      pstime = year + \"-\" + month + \"-\" + day + \" \" + (parseInt(h) + 1);\
    var message = \"箱号:\" + upd.CNTR + \"已被安排给您托送\" +\
      \"\\\\
计划送货时间:\" + pstime +\
      \"\\\\
