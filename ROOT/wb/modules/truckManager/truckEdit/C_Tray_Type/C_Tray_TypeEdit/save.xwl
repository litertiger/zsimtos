  ids.push({\
    ACCEPT_ID: id\
  });\
});\
request.setAttribute('create', Wb.encode(recs)); //重置create记录\
app.update({\
  tableName: 'C_TRAY_TYPE',\
  jndi:'jdbc/basecode'\
});\
app.send(ids); //把id列表发送到客户端以同步到表格\
