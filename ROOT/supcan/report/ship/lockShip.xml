<?xml version="1.0" encoding="utf-8"?> 
<TreeList>
  <Properties curselmode="rows" isShowRuler="true" rowHeight="35" editable="false" isFixTotalRow ="true" subtotalBgColor="#d3d3d3">
    
  </Properties>
  <Fonts>
    <Font/>
    <Font faceName="Verdana" weight="700"/>
  </Fonts>
  <Cols>
    

    <Col name="SEQID"  isCheckboxOnly="true" width="40" align="center"></Col>
    <Col name="ID"  editAble="false" width="0" align="center"></Col>
    <Col name="SD"  edittype="checkbox" editAble="false"   width="75" align="center">锁定</Col>
         <Col name="ETA" editAble="false" width="150" align="center" datatype="date">时间</Col> 

    <Col name="VESSEL_NAME" editAble="false" width="80" align="center" 
       totalExpress="='总份数:'+@rows+'份'"   >船名</Col>
     <Col name="VOYAGE_NO" editAble="false" width="100" align="center">航次</Col>
     <Col name="I_E"  edittype="droplist" droplistID="ieIdList" editAble="false" width="90" align="center">进出口</Col>
     <Col name="TRADE_ID" edittype="droplist" droplistID="tradeIdList" editAble="false" width="90" align="center">内外贸</Col>
  </Cols>
  <DropLists>
    <DropList id="ieIdList">
      <item key="I">进口</item>
      <item key="E">出口</item>
    </DropList>
    <DropList id="efIdList">
      <item key="E">空箱</item>
      <item key="F">重箱</item>
    </DropList>
    <DropList id="tradeIdList">
      <item key="1">外贸</item>
      <item key="2">内贸</item>
    </DropList>
  </DropLists>
</TreeList>