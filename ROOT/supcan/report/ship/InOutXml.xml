<?xml version="1.0" encoding="utf-8"?> 
<TreeList>
  <Properties curselmode="rows" isShowRuler="true" rowHeight="35" editable="true" isFixTotalRow ="true" subtotalBgColor="#d3d3d3">
    
  </Properties>
  <Fonts>
    <Font/>
    <Font faceName="Verdana" weight="700"/>
  </Fonts>
  <Cols>
    

    <Col name="SEQID"  isCheckboxOnly="true" width="40" align="center"></Col>
     <Col name="ID"  ishide="true" width="40" align="center"></Col>
    <Col name="CNTR_OWNER_COD" editAble="false" width="80" align="center" edittype="droplist" droplistID="agList"
       totalExpress="='总份数:'+@rows+'份'"   >箱主</Col>
       <Col name="CNTR_OPERATOR_COD" editAble="false" width="80" align="center" edittype="droplist" droplistID="agList"
       >驳船</Col>
        <Col name="IN_OUT_TIME" editAble="false" width="150" align="center" datatype="datetime">进出门时间</Col>
      <Col name="IN_OUT_TYPE" editAble="false" width="80" align="center">进出状态</Col>
    <Col name="SHIP_ORDER" editAble="false" width="120" align="center">订舱号</Col>

    <Col name="CNTR" editAble="false" width="90" align="center">箱号</Col>
    <Col name="CNTR_SIZE" editAble="false" width="90" align="center">尺寸</Col>
    <Col name="CNTR_TYPE" editAble="false" width="90" align="center">类型</Col>
    <Col name="I_E_ID" editAble="false" width="90" edittype="droplist" droplistID="ieIdList" align="center">进出口</Col>

    <Col name="CNTR_ISO" editAble="false" width="55" align="center" >箱ISO</Col>
    <Col name="E_F_ID" editAble="false" width="50" edittype="droplist" droplistID="efIdList" align="center">空重</Col>
    <Col name="CNTR_OPERATOR" editAble="false" width="80" align="center">驳船</Col>
    <Col name="POL" editAble="false" width="80" align="center" edittype="droplist" droplistID="portList">装货港</Col>
    <Col name="POD" editAble="false" width="80" align="center" edittype="droplist" droplistID="portList">卸货港</Col>

    <Col name="GOODS_NOTES" editAble="false" width="120" align="center">货名</Col>
    <Col name="WEIGHT" editAble="false" width="80" align="center">重量</Col>
    <Col name="PIECES" editAble="false" width="50" align="center" >件数</Col>
    <Col name="BILL_NO" editAble="true" width="120" align="center">提单号码</Col>
    <Col name="TRADE_ID" editAble="false" width="80" edittype="droplist" droplistID="tradeIdList"  align="center">内外贸</Col>
    <Col name="SEAL_NO" editAble="false" width="40" align="center" >铅封号</Col>
   

  </Cols>
  <DropLists>
    <DropList id="ieIdList">
      <item key="I">进口</item>
      <item key="E">出口</item>
    </DropList>
    <DropList id="efIdList">
      <item key="E">空箱</item>
      <item key="F">重箱</item>
    </DropList>
    <DropList id="tradeIdList">
      <item key="1">外贸</item>
      <item key="2">内贸</item>
    </DropList>
    <Droplist id="portList" treelist="/m?xwl=system/common/report/selectCod&tableName=C_PORT" DataCol="K" DisplayCol="V"/>
<Droplist id="agList" treelist="/m?xwl=system/common/report/selectCod&tableName=AG_SHORT" DataCol="K" DisplayCol="V"/>
  <!--  
    <Droplist id="ptList" treelist="/m?xwl=system/common/report/selectCod&tableName=PT_TYPE" DataCol="K" DisplayCol="V"/>
    <Droplist id="wkList" treelist="/m?xwl=system/common/report/selectCod&tableName=WORK_WAY" DataCol="K" DisplayCol="V"/>
    <Droplist id="carList" treelist="/m?xwl=system/common/report/selectCod&tableName=C_TRUCK" DataCol="K" DisplayCol="V"/>
    <Droplist id="addressList" treelist="/m?xwl=system/common/report/selectCod&tableName=C_ADDRESS" DataCol="K" DisplayCol="V"/>
    <Droplist id="lichengList" treelist="/m?xwl=system/common/report/selectCod&tableName=C_DISTANCE" DataCol="K" DisplayCol="V"/>
-->
  </DropLists>
</TreeList>