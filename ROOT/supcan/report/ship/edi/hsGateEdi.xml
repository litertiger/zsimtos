<?xml version="1.0" encoding="utf-8"?> 
<TreeList>
  <Properties curselmode="rows" isShowRuler="true" rowHeight="35" editable="false" isFixTotalRow ="true" subtotalBgColor="#d3d3d3">
    
  </Properties>
  <Fonts>
    <Font/>
    <Font faceName="Verdana" weight="700"/>
  </Fonts>
  <Cols>
    

    <Col name="SEQID"  isCheckboxOnly="true" width="40" align="center"></Col>
    <Col name="FORM_ID"   width="180" align="center">ID</Col>
    <Col name="CAR_NO"   ishide='true' width="140" align="center">车号</Col>
    <Col name="CONTA_ID" editAble="false" width="80" align="center" 
       totalExpress="='总份数:'+@rows+'份'"   >箱号</Col>
    <Col name="GATE_TIME" editAble="false" width="150" align="center" datatype="datetime">进出门时间</Col>
    <Col name="CNTR_SIZE" editAble="false" width="90" align="center">尺寸</Col>
    <Col name="CNTR_TYPE" editAble="false" width="90" align="center">类型</Col>
    <Col name="I_E_FLAG" editAble="false" width="90" edittype="droplist" droplistID="ieIdList" align="center">进出口</Col>
    <Col name="EMPTY_CONTA_MARK" editAble="false" width="50" edittype="droplist" droplistID="efIdList" align="center">空重</Col>
    <Col name="BILL_NO" editAble="false" width="120" align="center">提单号码</Col>
    <Col name="SEAL_NO" editAble="false" width="40" align="center" >铅封号</Col>
   

  </Cols>
  <DropLists>
    <DropList id="ieIdList">
      <item key="I">进口</item>
      <item key="E">出口</item>
    </DropList>
    <DropList id="efIdList">
      <item key="E">空箱</item>
      <item key="F">重箱</item>
    </DropList>
    <DropList id="tradeIdList">
      <item key="1">外贸</item>
      <item key="2">内贸</item>
    </DropList>
  </DropLists>
</TreeList>