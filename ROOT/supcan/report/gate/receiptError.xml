    <Col name="IN_CAR" width="60" align="center">拖车</Col>
    <Col name="CAR_TEAM" edittype="droplist" droplistID="carteamList" width="140" align="center">车队</Col>
    <Col name="SHIPPER_WAY" width="70" align="center">承运方式</Col>
    <Col name="PT_TYPE" edittype="droplist" droplistID="ptList" width="100" align="center">车辆性质</Col>
