<?xml version="1.0" encoding="utf-8"?> 
<TreeList>
  <Properties curselmode="rows" isShowRuler="true" rowHeight="35" editable="true" isFixTotalRow ="true" subtotalBgColor="#d3d3d3">
  </Properties>
  <Fonts>
    <Font/>
    <Font faceName="Verdana" weight="700"/>
  </Fonts>
  <Cols>
<!--     <Col name="CNTR_OPERATOR" editAble="false" width="100" align="center" totalExpress="='合计：'">船公司</Col> -->
    <Col name="BILL_NO" editAble="false" width="100" align="center">提单号</Col>
    <Col name="CNTR" editAble="false" width="100" align="center">箱号</Col>
    <Col name="CNTR_SIZE" editAble="false" width="100" align="center">尺寸</Col>
    <Col name="CNTR_TYPE" editAble="false" width="100" align="center">箱型</Col>
    <Col name="GTAREWT" editAble="false" width="100" align="center">柜自重</Col>
    <Col name="WEIGHTIME" editAble="false" width="100" align="center" datatype="date">过磅时间</Col>
    <Col name="VID" editAble="false" width="100" align="center" isHide="true">船舶编号</Col>
    <Col name="E_F_ID" editAble="false" width="100" align="center" edittype="droplist" droplistID="efIdList">空重</Col>
    <Col name="TRUCK_NO" editAble="false" width="100" align="center">车牌号</Col>
    <Col name="TRAY_NO" editAble="false" width="100" align="center">托盘号</Col>
    <Col name="TRUCKWT" editAble="false" width="100" align="center">拖头重量</Col>
    <Col name="TRAYWT" editAble="false" width="100" align="center">托卡重量</Col>
    <Col name="TOTALWT" editAble="false" width="100" align="center">总重量</Col>
    <Col name="GROSSWT" editAble="false" width="100" align="center">货重</Col>
    <Col name="TENANCY_ID" editAble="false" width="100" align="center" isHide="true">租户</Col>
    <Col name="PHASE" editAble="false" width="100" align="center" edittype="droplist" droplistID="phaseList">任务状态</Col>
    <Col name="I_E_ID" editAble="false" width="100" align="center" edittype="droplist" droplistID="ieIdList">进出口</Col>
    <Col name="GOODS" editAble="false" width="100" align="center">货物</Col>
    <Col name="ISOUT" editAble="false" width="100" align="center" isCheckboxOnly="true">是否外磅</Col>  
    <Col name="ISGROCERY" editAble="false" width="100" align="center" isCheckboxOnly="true">是否散货</Col>
<!--     <Col name="SHIPPER" editAble="false" width="100" align="center">货主</Col> -->
<!--     <Col name="GKNO" editAble="false" width="100" align="center">磅号</Col> -->
<!--     <Col name="WEIGHT_TYPE" editAble="false" width="100" align="center" edittype="droplist" droplistID="weightTypeList">过磅类型</Col> -->
  </Cols>
  <DropLists>
    <DropList id="ieIdList">
      <item key="I">进口</item>
      <item key="E">出口</item>
    </DropList>
    <DropList id="efIdList">
      <item key="E">空箱</item>
      <item key="F">重箱</item>
    </DropList>
    <DropList id="phaseList">
      <item key="PLN">计划</item>
      <item key="SND">激活</item>
      <item key="EXE">执行</item>
      <item key="FIN">完成</item>
    </DropList>
    <DropList id="weightTypeList">
      <item key="CHK">查验过磅</item>
      <item key="GAT">闸口过磅</item>
    </DropList>
    <Droplist id="ptList" treelist="/m?xwl=system/common/report/selectCod&tableName=PT_TYPE" DataCol="K" DisplayCol="V"/>
    <Droplist id="portList" treelist="/m?xwl=system/common/report/selectCod&tableName=C_PORT" DataCol="K" DisplayCol="V"/>
    <Droplist id="addressList" treelist="/m?xwl=system/common/report/selectCod&tableName=C_ADDRESS" DataCol="K" DisplayCol="V"/>
    <Droplist id="lichengList" treelist="/m?xwl=system/common/report/selectCod&tableName=C_DISTANCE" DataCol="K" DisplayCol="V"/>
  </DropLists>
</TreeList>