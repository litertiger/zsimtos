<?xml version="1.0" encoding="utf-8"?> 
<TreeList>
  <Properties curselmode="rows" isShowRuler="true" isFixTotalRow ="true" rowHeight="35" editable="false" subtotalBgColor="#d3d3d3">
  </Properties>
  <Fonts>
    <Font/>
    <Font faceName="Verdana" weight="700"/>
  </Fonts>
  <Cols>
    <Col name="CNTR" editAble="false" width="85" align="center">箱号</Col>
    <Col name="SHIPPER_CONSIGNEE" editAble="false" width="120" align="center">货主</Col>
    <Col name="CNTR_OPERATOR_COD" editAble="false" width="80" align="center" edittype="droplist" droplistID="agList">驳船</Col>
    <Col name="CNTR_SIZE" editAble="false" width="40" align="center">尺寸</Col>
    <Col name="CNTR_TYPE" editAble="false" width="40" align="center">箱型</Col>
    <Col name="BILL_NO" editAble="false" width="120" align="center">提单号码</Col>
    <Col name="IN_CAR" editAble="false" width="80" align="center">提箱拖头</Col>
    <Col name="IN_DRIVER_ID" editAble="false" width="100" align="center" edittype="droplist" droplistID="driverList">提箱司机</Col>
    <Col name="IN_CAMPI" editAble="false" width="80" align="center">提箱拖卡</Col>
    <Col name="OUT_CAR" editAble="false" width="80" align="center">还箱拖头</Col>
    <Col name="OUT_DRIVER_ID" editAble="false" width="100" align="center" edittype="droplist" droplistID="driverList">还箱司机</Col>
        <Col name="GATEIN_TIME_T1" width="140" align="center">指令发送时间</Col>
    <Col name="UPDATED_ON" width="140" align="center">指令完成时间</Col>
    <Col name="ZTIME" editAble="false"  width="80" align="center">装车时长</Col>
    <Col name="GATEIN_TIME_S" editAble="false" width="150" align="center">进闸时间</Col>
 
    <Col name="TIMELONG" editAble="false"  width="80" align="center">进出闸时长</Col>
       <Col name="GATEOUT_TIME_T" editAble="false" width="140" align="center">出闸时间</Col>
 
    <Col name="FROM_POS" editAble="false"  width="80" align="center">提箱位置</Col>
    <Col name="GATEIN_NO_T" editAble="false" width="70" align="center" edittype="droplist" droplistID="cfsList">提箱闸口</Col>
    <Col name="GATEIN_NO_S" editAble="false" width="70" align="center" edittype="droplist" droplistID="cfsList">还箱闸口</Col>
    <Col name="WORK_TYPE" editAble="false" edittype="droplist" droplistID="wtList" width="60" align="center">类型</Col>
    <Col name="TRADE_ID" editAble="false" edittype="droplist" droplistID="tradeList" width="60" align="center">内外贸</Col>
    <Col name="I_E_ID" editAble="false" edittype="droplist" droplistID="ieIdList" width="70" align="center">进出口</Col>
    <Col name="CNTR_OWNER_COD" editAble="false" width="60" align="center">箱主</Col>
    <Col name="CAR_TEAM" editAble="false" edittype="droplist" droplistID="carteamList" width="140" align="center">车队</Col>
    <Col name="GATEIN_TIME_T" width="140" align="center">打单时间</Col>

  </Cols>
  <DropLists>
    <DropList id="ieIdList">
      <item key="I">进口</item>
      <item key="E">出口</item>
    </DropList>
    <DropList id="tradeList">
      <item key="2">内贸</item>
      <item key="1">外贸</item>
    </DropList>
    <DropList id="billTypeList">
      <item key="C">整柜</item>
      <item key="E">吉柜</item>
      <item key="P">拼柜</item>
      <item key="S">散货</item>
    </DropList>   
    <DropList id="traiIdList">
      <item key="1">外贸</item>
      <item key="2">内贸</item>
    </DropList>
    <DropList id="efList">
      <item key="E">空箱</item>
      <item key="F">重箱</item>
    </DropList>
    <Droplist id="cfsList" treelist="/m?xwl=system/common/report/selectCod&tableName=C_CFS" DataCol="K" DisplayCol="V"/>
    <Droplist id="wtList" treelist="/m?xwl=system/common/report/selectCod&tableName=WORK_WAY" DataCol="K" DisplayCol="V"/>
    <Droplist id="carteamList" treelist="/m?xwl=system/common/report/selectCod&tableName=TU_CUSTOMER" DataCol="K" DisplayCol="V"/>
    <Droplist id="driverList" treelist="/m?xwl=system/common/report/selectCod&tableName=C_DRIVER" DataCol="K" DisplayCol="V"/>
    <Droplist id="agList" treelist="/m?xwl=system/common/report/selectCod&tableName=AG_SHORT" DataCol="K" DisplayCol="V"/>
  </DropLists>
</TreeList>